<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.pankaj.javabeans.MobieDetails" table="mobie_details">
		<id name="mobId" type="java.lang.Integer">
			<column name="mob_id" />
			<generator class="identity" />
		</id>
		<property name="mobName" type="java.lang.String">
			<column name="mob_name" length="40" not-null="true" />
		</property>
		<property name="mobLanguage" type="java.lang.String">
			<column name="mob_language" length="30" not-null="true" />
		</property>
		<property name="price" type="java.lang.Integer">
			<column name="price" length="5" not-null="true" />
		</property>
	</class>
</hibernate-mapping>